{
	"name": "@interval.so/api",
	"version": "0.0.0",
	"private": true,
	"license": "UNLICENSED",
	"type": "module",
	"imports": {
		"#controllers/*": "./app/controllers/*.js",
		"#exceptions/*": "./app/exceptions/*.js",
		"#models/*": "./app/models/*.js",
		"#mails/*": "./app/mails/*.js",
		"#services/*": "./app/services/*.js",
		"#listeners/*": "./app/listeners/*.js",
		"#events/*": "./app/events/*.js",
		"#middleware/*": "./app/middleware/*.js",
		"#validators/*": "./app/validators/*.js",
		"#providers/*": "./providers/*.js",
		"#policies/*": "./app/policies/*.js",
		"#abilities/*": "./app/abilities/*.js",
		"#database/*": "./database/*.js",
		"#start/*": "./start/*.js",
		"#config/*": "./config/*.js"
	},
	"scripts": {
		"build": "node ace build",
		"dev": "PORT=3001 node ace serve --hmr",
		"dev:debug": "PORT=3001 node --inspect ace serve --hmr",
		"migrate:apply": "drizzle-kit migrate",
		"migrate:create": "drizzle-kit generate",
		"start": "node bin/server.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@adonisjs/auth": "9.3.2",
		"@adonisjs/bouncer": "3.1.5",
		"@adonisjs/core": "6.17.2",
		"@adonisjs/cors": "2.2.1",
		"@adonisjs/redis": "9.2.0",
		"@adonisjs/session": "7.5.1",
		"@jonahsnider/util": "11.0.0",
		"@paralleldrive/cuid2": "2.2.2",
		"@rlanz/sentry": "^0.2.0",
		"@sentry/node": "8.52.0",
		"@sentry/profiling-node": "8.52.0",
		"@simplewebauthn/server": "10.0.1",
		"@trpc/server": "11.0.0-rc.630",
		"@vinejs/vine": "3.0.1",
		"convert": "5.8.0",
		"crypto-random-string": "5.0.0",
		"date-fns": "4.1.0",
		"drizzle-kit": "0.30.5",
		"drizzle-orm": "0.40.1",
		"postgres": "3.4.5",
		"posthog-node": "4.10.1",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.2",
		"superjson": "2.2.2",
		"ts-pattern": "5.6.2",
		"ws": "8.18.1",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@adonisjs/assembler": "7.8.2",
		"@adonisjs/tsconfig": "1.4.0",
		"@simplewebauthn/types": "10.0.0",
		"@swc/core": "1.10.1",
		"@types/node": "22.13.10",
		"@types/pg": "8.11.10",
		"@types/ws": "^8.5.13",
		"dotenv": "16.4.7",
		"execa": "9.5.2",
		"hot-hook": "patch:hot-hook@npm%3A0.3.0#~/.yarn/patches/hot-hook-npm-0.3.0-0c210b56e7.patch",
		"pino-pretty": "13.0.0",
		"prettier": "3.5.3",
		"ts-node": "10.9.2",
		"typescript": "5.8.2"
	},
	"packageManager": "yarn@4.7.0",
	"engines": {
		"node": "22.x"
	},
	"hotHook": {
		"boundaries": [
			"./app/controllers/**/*.ts",
			"./app/routers/**/*.ts",
			"./app/**/*_controller.ts",
			"./app/**/*_router.ts",
			"./app/middleware/*.ts"
		]
	}
}
