{
  "name": "@hours.frc.sh/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "PORT=3001 node ace serve --hmr",
    "typecheck": "tsc --noEmit",
    "migrate:create": "drizzle-kit generate",
    "migrate:apply": "drizzle-kit migrate"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.6.1",
    "@adonisjs/tsconfig": "^1.3.0",
    "@swc/core": "^1.5.7",
    "@types/node": "^20.12.12",
    "@types/pg": "^8",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.22.1",
    "execa": "9.1.0",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^20.6.0",
    "@trpc/server": "11.0.0-rc.382",
    "@vinejs/vine": "^2.0.0",
    "drizzle-orm": "0.31.0",
    "pg": "8.11.5",
    "postgres": "3.4.4",
    "reflect-metadata": "^0.2.2"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/routers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  }
}
