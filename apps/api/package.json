{
	"name": "@interval.so/api",
	"version": "0.0.0",
	"private": true,
	"license": "UNLICENSED",
	"type": "module",
	"imports": {
		"#controllers/*": "./app/controllers/*.js",
		"#exceptions/*": "./app/exceptions/*.js",
		"#models/*": "./app/models/*.js",
		"#mails/*": "./app/mails/*.js",
		"#services/*": "./app/services/*.js",
		"#listeners/*": "./app/listeners/*.js",
		"#events/*": "./app/events/*.js",
		"#middleware/*": "./app/middleware/*.js",
		"#validators/*": "./app/validators/*.js",
		"#providers/*": "./providers/*.js",
		"#policies/*": "./app/policies/*.js",
		"#abilities/*": "./app/abilities/*.js",
		"#database/*": "./database/*.js",
		"#start/*": "./start/*.js",
		"#config/*": "./config/*.js"
	},
	"scripts": {
		"build": "node ace build",
		"dev": "PORT=3001 node ace serve --hmr",
		"dev:debug": "PORT=3001 node --inspect ace serve --hmr",
		"migrate:apply": "drizzle-kit migrate",
		"migrate:create": "drizzle-kit generate",
		"start": "node bin/server.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@adonisjs/auth": "9.5.1",
		"@adonisjs/bouncer": "3.1.6",
		"@adonisjs/core": "6.19.3",
		"@adonisjs/cors": "2.2.1",
		"@adonisjs/redis": "9.2.0",
		"@adonisjs/session": "7.7.1",
		"@jonahsnider/util": "13.0.0",
		"@paralleldrive/cuid2": "3.3.0",
		"@rlanz/sentry": "^0.3.3",
		"@sentry/node": "10.38.0",
		"@sentry/profiling-node": "10.38.0",
		"@simplewebauthn/server": "13.1.2",
		"@trpc/server": "11.8.1",
		"@vinejs/vine": "4.2.0",
		"convert": "6.0.0",
		"crypto-random-string": "5.0.0",
		"date-fns": "4.1.0",
		"drizzle-kit": "0.31.8",
		"drizzle-orm": "0.45.1",
		"postgres": "3.4.8",
		"posthog-node": "5.24.2",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.2",
		"superjson": "2.2.6",
		"ts-pattern": "5.9.0",
		"ws": "8.19.0",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@adonisjs/assembler": "7.8.2",
		"@adonisjs/tsconfig": "1.4.1",
		"@swc/core": "1.15.10",
		"@types/node": "24.10.9",
		"@types/pg": "8.16.0",
		"@types/ws": "8.18.1",
		"dotenv": "17.2.3",
		"execa": "9.6.1",
		"hot-hook": "patch:hot-hook@npm%3A0.3.0#~/.yarn/patches/hot-hook-npm-0.3.0-0c210b56e7.patch",
		"pino-pretty": "13.1.3",
		"prettier": "3.7.4",
		"ts-node": "10.9.2",
		"typescript": "5.9.3"
	},
	"packageManager": "yarn@4.12.0",
	"engines": {
		"node": "24.x"
	},
	"hotHook": {
		"boundaries": [
			"./app/controllers/**/*.ts",
			"./app/routers/**/*.ts",
			"./app/**/*_controller.ts",
			"./app/**/*_router.ts",
			"./app/middleware/*.ts"
		]
	}
}
