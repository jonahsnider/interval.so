{
	"name": "@interval.so/api",
	"version": "0.0.0",
	"private": true,
	"license": "UNLICENSED",
	"type": "module",
	"imports": {
		"#controllers/*": "./app/controllers/*.js",
		"#exceptions/*": "./app/exceptions/*.js",
		"#models/*": "./app/models/*.js",
		"#mails/*": "./app/mails/*.js",
		"#services/*": "./app/services/*.js",
		"#listeners/*": "./app/listeners/*.js",
		"#events/*": "./app/events/*.js",
		"#middleware/*": "./app/middleware/*.js",
		"#validators/*": "./app/validators/*.js",
		"#providers/*": "./providers/*.js",
		"#policies/*": "./app/policies/*.js",
		"#abilities/*": "./app/abilities/*.js",
		"#database/*": "./database/*.js",
		"#start/*": "./start/*.js",
		"#config/*": "./config/*.js"
	},
	"scripts": {
		"build": "node ace build",
		"dev": "PORT=3001 node ace serve --hmr",
		"dev:debug": "PORT=3001 node --inspect ace serve --hmr",
		"migrate:apply": "drizzle-kit migrate",
		"migrate:create": "drizzle-kit generate",
		"start": "node bin/server.js",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@adonisjs/auth": "9.2.3",
		"@adonisjs/bouncer": "3.1.3",
		"@adonisjs/core": "6.12.1",
		"@adonisjs/cors": "2.2.1",
		"@adonisjs/redis": "9.1.0",
		"@adonisjs/session": "7.4.2",
		"@jonahsnider/util": "11.0.0",
		"@paralleldrive/cuid2": "2.2.2",
		"@rlanz/sentry": "^0.1.0",
		"@sentry/node": "8.26.0",
		"@sentry/profiling-node": "8.26.0",
		"@simplewebauthn/server": "10.0.1",
		"@trpc/server": "11.0.0-rc.485",
		"@vinejs/vine": "2.1.0",
		"convert": "5.3.0",
		"crypto-random-string": "5.0.0",
		"date-fns": "3.6.0",
		"drizzle-kit": "0.24.1",
		"drizzle-orm": "0.33.0",
		"postgres": "3.4.4",
		"posthog-node": "4.1.1",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.1",
		"superjson": "2.2.1",
		"ts-pattern": "5.3.1",
		"ws": "8.18.0",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@adonisjs/assembler": "7.7.0",
		"@adonisjs/tsconfig": "1.3.0",
		"@simplewebauthn/types": "10.0.0",
		"@swc/core": "1.7.14",
		"@types/node": "20.16.1",
		"@types/pg": "8.11.6",
		"@types/ws": "^8",
		"dotenv": "16.4.5",
		"execa": "9.3.1",
		"hot-hook": "0.2.6",
		"pino-pretty": "11.2.2",
		"prettier": "3.3.3",
		"ts-node": "10.9.2",
		"typescript": "5.5.4"
	},
	"packageManager": "yarn@4.4.0",
	"engines": {
		"node": "20.x"
	},
	"hotHook": {
		"boundaries": [
			"./app/controllers/**/*.ts",
			"./app/routers/**/*.ts",
			"./app/**/*_controller.ts",
			"./app/**/*_router.ts",
			"./app/middleware/*.ts"
		]
	}
}
