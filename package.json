	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"prettier": "3.6.2",
		"prettier-plugin-packagejson": "2.5.19",
		"turbo": "2.5.6",
		"vercel": "47.0.5"
	},
	"packageManager": "yarn@4.9.4",
	"engines": {
		"node": "22.x"
	},
	"trustedDependencies": [
		"@biomejs/biome"
	]
